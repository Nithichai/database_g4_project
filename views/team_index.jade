extends layout

block content
  .w3-container.w3-padding-64.w3-animate-opacity
    - each team in data
      .w3-third.w3-padding-large
          .w3-card-4.w3-hover-light-grey
            .w3-container.w3-center
              img(src='/images/#{team.team}.PNG', style="width:100%")
            .w3-container.w3-theme.w3-center
              h3 #{team.team}

  .w3-container.w3-black.w3-center.w3-padding-64
      a.w3-button.w3-border.w3-border-white(href='#').w3-large Back to Top

